# Meninet.Filesystem
FROM ubuntu:16.04

RUN apt update

WORKDIR /corner
COPY ./install/install.sh /corner
RUN chmod 777 install.sh

COPY ./seaweedfs/srv/weed /usr/bin

#fs master
EXPOSE 10333:10333

#fs volume
EXPOSE 8083:8083
EXPOSE 18083:18083
ENV CORNER_ASSET_DRIVER="seaweedfs"

ENTRYPOINT ./install.sh
